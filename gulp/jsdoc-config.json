{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "template": "templates/default",
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true

  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}